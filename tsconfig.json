{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@db/*": ["electron/main/db/*"],
      "@controllers/*": ["controllers/*"]
    },
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "module": "commonjs",
    "target": "ES2020",
    "noEmit": true,
    "skipLibCheck": true
  }
}
